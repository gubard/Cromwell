<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:m="clr-namespace:Cromwell.Models">
    <Style Selector="ListBox.credential-view">
        <Setter Property="ItemTemplate">
            <DataTemplate DataType="m:CredentialNotify">
                <TextBlock Classes="alignment-left-center text-wrap" Text="{Binding Name}" />
            </DataTemplate>
        </Setter>
    </Style>
</Styles>